{
    "rules": {
        "indent": [ "error", 2 ],
        "quotes": [ "error", "double" ],
        "linebreak-style": [ 2, "unix" ],
        "semi": [ "error", "never" ],
        "no-unused-vars": "warn"
    },
    "env": {
        "es6": true,
        "node": true
    },
    "parserOptions": {,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    }
}
